{
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": true
    },
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.padding.bottom": 250,
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": false
    },
    "editor.scrollBeyondLastLine": false,
    "editor.wordSeparators": "./\\<)\"'>:,.;<>~!@# ○$%^&*|+=[]{}`~?．。，、（）「」［］｛｝《》てにをはがのともへでや"
  },
  "cSpell.words": [
    "__alltraps",
    "ASID",
    "auipc",
    "binutils",
    "bitflags",
    "clippy",
    "dccache",
    "dealloc",
    "ebss",
    "ecall",
    "ekernel",
    "GETCHAR",
    "icache",
    "incbin",
    "inlateout",
    "kstack",
    "mutatorily",
    "objcopy",
    "putchar",
    "repr",
    "riscv",
    "rustfmt",
    "rustup",
    "sbss",
    "scause",
    "sepc",
    "SFENCE",
    "sret",
    "sscratch",
    "sstatus",
    "stie",
    "stimer",
    "stval",
    "stvec",
    "syscall",
    "syscalls",
    "unborrowed",
    "uniprocessor",
    "userspace",
    "Virt",
    "xstate"
  ],
  "files.associations": {
    "*.asm": "riscv",
    "*.S": "riscv"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "files.exclude": {
    "**/.dccache": true,
    "**/.gdb_history": true,
    "**/last-qemu": true
  },
  "markdownlint.config": {
    "default": true,
    "MD024": false,
    "MD025": false,
    "MD033": false,
    "no-hard-tabs": false
  },
  "rust-analyzer.cargo.features": [
    "board_qemu"
  ]
}
