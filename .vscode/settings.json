{
  "files.associations": {
    "*.asm": "riscv",
    "*.S": "riscv",
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "files.exclude": {
    "**/.dccache": true,
  },
  "rust-analyzer.cargo.features": [
    "board_qemu"
  ],
  "cSpell.words": [
    "__alltraps",
    "ASID",
    "dccache",
    "ebss",
    "ecall",
    "GETCHAR",
    "icache",
    "inlateout",
    "putchar",
    "repr",
    "riscv",
    "sbss",
    "scause",
    "sepc",
    "SFENCE",
    "sret",
    "sscratch",
    "sstatus",
    "stval",
    "stvec",
    "syscall",
    "syscalls",
    "uniprocessor",
    "userspace",
    "xstate"
  ],
}
